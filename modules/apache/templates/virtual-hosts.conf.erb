# Virtual Host para servidor Apache donde vamos alojar wordpress
<VirtualHost *:80>
  ServerAdmin webmaster@localhost
  DocumentRoot <%= @base_site %>

  <Directory <%= @base_site %>>
    Require all granted
  </Directory>

  ErrorLog ${APACHE_LOG_DIR}/error.log
  CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
